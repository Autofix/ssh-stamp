{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "probe-rs-debug",
            "request": "launch",
            "name": "Launch",
            "cwd": "${workspaceFolder}",
            "chip": "esp32c6",
            "probe": "10c4:ea60",
            "flashingConfig": {
                "flashingEnabled": true,
                "haltAfterReset": true,
                "formatOptions": {
                    "binaryFormat": "elf"
                }
            },
            "coreConfigs": [
                {
                    "coreIndex": 0,
                    "programBinary": "target/riscv32imac-unknown-none-elf/debug/esp-ssh-rs",
                    "svdFile": "${workspaceFolder}/esp32c6.svd"
                }
            ]
        },
        {
            "type": "probe-rs-debug",
            "request": "attach",
            "name": "Attach",
            "cwd": "${workspaceFolder}",
            "chip": "esp32c6",
            "probe": "10c4:ea60",
            "coreConfigs": [
                {
                    "coreIndex": 0,
                    "programBinary": "target/riscv32imac-unknown-none-elf/debug/esp-ssh-rs",
                    "svdFile": "${workspaceFolder}/esp32c6.svd"
                }
            ]
        }
    ]
}